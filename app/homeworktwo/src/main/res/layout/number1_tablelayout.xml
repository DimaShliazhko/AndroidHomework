            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:foregroundGravity="center"
            android:text="Dinning">

        </TextView>
    </TableRow>

    <TableRow android:layout_weight="0.1">>

